{
    "version": "1.0.0",
    "configurations": [
        {
            "name": "CNK Frontend",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "deploy/frontend/app/main.py",
                "FLASK_ENV": "development"
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload"
            ],
            "debugOptions": [
                "RedirectOutput"
            ]
        }
    ]
}
